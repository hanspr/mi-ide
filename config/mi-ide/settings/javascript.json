{
    "autoclose": true,
    "autoindent": true,
    "blockclose": "^[}\\])]",
    "blockinter": "^[}\\])].+?[{[(]$|:$",
    "blockopen": "[{[\\(]$",
    "comment": "//",
    "eofnewline": true,
    "fileformat": "unix",
    "findfuncregex": "^\\s*function\\s+%word%[\\s\\(]|%word%\\s*=\\s*(?:function|\\(?:.*?\\)\\s*=\u003e)[\\s\\(]",
    "indentchar": " ",
    "keepautoindent": true,
    "matchbrace": true,
    "rmtrailingws": true,
    "smartindent": true,
    "smartpaste": true,
    "softwrap": false,
    "tabindents": true,
    "tabmovement": true,
    "tabsize": 4,
    "tabstospaces": true,
    "useformatter": true
}
