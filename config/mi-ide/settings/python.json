{
    "autoclose": true,
    "autoindent": true,
    "autoreload": true,
    "blockclose": "pass#$",
    "blockinter": "el(?:se|if)",
    "blockopen": ":$",
    "comment": "#",
    "eofnewline": false,
    "fileformat": "unix",
    "indentchar": " ",
    "keepautoindent": true,
    "matchbrace": true,
    "rmtrailingws": false,
    "smartindent": false,
    "smartpaste": true,
    "softwrap": false,
    "tabindents": false,
    "tabmovement": true,
    "tabsize": 4,
    "tabstospaces": true,
    "useformatter": true
}
